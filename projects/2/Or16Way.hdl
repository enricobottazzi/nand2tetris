// File name: projects/2/Or16Way.hdl
/**
 * 16-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[15]
 */
CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
    Or8Way(in=in[0..7], out=out1);
    Or8Way(in=in[8..15], out=out2);
    Or(a=out1, b=out2, out=out);
}